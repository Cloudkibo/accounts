<!-- views/page/login.ejs -->

<!-- begin:: Page -->
<div class="m-grid m-grid--hor m-grid--root m-page">
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-grid--tablet-and-mobile m-grid--hor-tablet-and-mobile m-login m-login--1 m-login--singin" id="m_login">
            <div class="m-grid__item m-grid__item--order-tablet-and-mobile-2 m-login__aside">
                <div class="m-stack m-stack--hor m-stack--desktop">
                    <div class="m-stack__item m-stack__item--fluid">
                        <div class="m-login__wrapper">
                            <%- include('../partials/header', { heading: 'Team Sign Up' }); %>
                                <div class="form-group m-form__group">
                                    <input class="form-control m-input" type="text" placeholder="Name" name="name" id="name" autocomplete="off" required>
                                </div>
                                <div class="form-group m-form__group">
                                    <input class="form-control m-input" type="text" placeholder="Workspace name e.g mycompany" name="domain" id="domain" autocomplete="off" required>
                                </div>
                                <div class="form-group m-form__group">
                                        <input class="form-control m-input" type="text" placeholder="Company Name" name="CompanyName" id="CompanyName" autocomplete="off" required>
                                    </div>
                                <div class="form-group m-form__group">
                                    <input class="form-control m-input" type="email" placeholder="Email" name="email" id="email" autocomplete="off" required>
                                </div>
                                <div class="form-group m-form__group">
                                    <input class="form-control m-input m-login__form-input--last" type="password" placeholder="Password" name="password" id="password" required>
                                </div>
                                <div class="form-group m-form__group">
                                    <input class="form-control m-input m-login__form-input--last" type="password" placeholder="Confirm Password" name="confirm_password" id="confirm_password" required>
                                </div>
                                <div class="form-group m-form__group">

                                    <div class="g-recaptcha" data-sitekey="6Ld8YIIUAAAAADf6THSwZmQ1Y9E_cHKDaV56H7Cn"></div>
                                </div>

                                <div class="form-group m-form__group">
                                    <label class="m-checkbox m-checkbox--state-success">
                                        <input type="checkbox" id="checkboxAgreement" name="checkboxAgreement">
                                        I've read and accept the <a href="#" data-toggle="modal" data-target="#m_modal_1">terms and conditions</a>
                                        <span></span>
                                    </label>
                                </div>
                                <div class="form-group m-form__group">
                                  <div id='alertMsg' style="color: red;"></div>
                                 </div>
                                <div class="m-login__form-action">
                                    <button id="m_login_signin_submit" type="button" class="btn btn-focus m-btn m-btn--pill m-btn--custom m-btn--air signUpBtn">
                                        Sign Up
                                    </button>
                                </div>
                        </div>
                    </div>
                    <% include ../partials/footer-signup %>
                </div>
            </div>
            <% include ../partials/side %>
        </div>
    </div>
    <% include ../partials/eula-modal %>
    <!-- end:: Page -->
