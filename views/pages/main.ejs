<!-- views/page/login.ejs -->

<!-- begin:: Page -->
<div class="m-grid m-grid--hor m-grid--root m-page">
        <div class="m-grid__item m-grid__item--fluid m-grid m-grid--ver-desktop m-grid--desktop m-grid--tablet-and-mobile m-grid--hor-tablet-and-mobile m-login m-login--1 m-login--singin" id="m_login">
            <div class="m-grid__item m-grid__item--order-tablet-and-mobile-2 m-login__aside">
                <div class="m-stack m-stack--hor m-stack--desktop">
                    <div class="m-stack__item m-stack__item--fluid">
                        <div class="m-login__wrapper">
                            <div class="m-login__logo">
                                <a href="#">
                                    <img src="http://cdn.cloudkibo.com/public/img/logo.png" style="max-width: 250px;">
                                </a>
                            </div>
                            <div class="m-login__signin">
                                <div class="m-login__head">
                                    <h3 class="m-login__title">
                                        Welcome To KiboPush
                                    </h3>
                                </div>
                            </div>
                        </div>
                    <div class="m-stack__item m-stack__item--center" style="text-align: center;">
                        <div class="m-login__form-action" style="margin: 25px;">
                            <a href=<%= buttonOne.url %>  class="btn btn-focus m-btn m-btn--pill m-btn--custom m-btn--air">
                                <%= buttonOne.name %>
                            </a>
                        </div>
                        <div class="m-login__form-action">
                            <a href=<%= buttonTwo.url %>  class="btn btn-focus m-btn m-btn--pill m-btn--custom m-btn--air">
                                <%= buttonTwo.name %>
                            </a>
                        </div>
                    </div>
                    </div>
                    <div class="m-stack__item m-stack__item--center">
                        <div class="m-login__account">
                            <span class="m-login__account-msg">
                                    Want to learn more about KiboPush ?  
                            </span>
                            &nbsp;&nbsp;
                            <a href="http://kibopush.com/faqs/" id="m_login_signup" class="m-link m-link--focus m-login__account-link">
                                Visit FAQ page
                            </a>
                        </div>
                    </div>
                </div>
            </div>
            <% include ../partials/side %>
        </div>
    </div>
    <!-- end:: Page -->
